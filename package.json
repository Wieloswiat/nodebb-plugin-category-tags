{
  "name": "nodebb-plugin-category-tags",
  "version": "0.2.4",
  "description": "A starter kit for quickly creating NodeBB plugins",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/oplik0/nodebb-plugin-category-tags"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "category-tags",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "oplik0",
    "email": "opliko.reg+npm@protonmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oplik0/nodebb-plugin-category-tags/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^7.1.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0"
  }
}